package practice;
import java.io.*;

import java.util.*;
public class Solution {

	public static void main(String []args)
	  {
		int count=1;
		Scanner sc = new Scanner(System.in);
		String s = sc.nextLine();
		int n = s.length();
		for(int i=0;i<n;i++)
		{
			if(64<(int)s.charAt(i)&(int)s.charAt(i)<90)
			{
				count++;
			}
		}
		System.out.println(count);
		
	  }   
}
